{
  "version": "3.41.0",
  "name": "@dc/ui-library",
  "description": "UI Library",
  "repository": {
    "type": "git",
    "url": "http://stash.dsg-i.com/projects/UP/repos/ui-library"
  },
  "directories": {
    "lib": "src"
  },
  "main": "src/index.js",
  "license": "ISC",
  "scripts": {
    "backstop": "./backstop.sh --configPath=backstopjs/master",
    "commitmsg": "node ./npm_scripts/commitlint.js",
    "start": "styleguidist server",
    "start:currys": "cross-env PORT=3001 cross-env THEME=currys npm start",
    "start:pcworld": "cross-env PORT=3002 cross-env THEME=pcworld npm start",
    "build": "concurrently -c blue,magenta -n currys,pcworld \"npm run build:currys\" \"npm run build:pcworld\"",
    "build:currys": "cross-env THEME=currys styleguidist build",
    "build:pcworld": "cross-env THEME=pcworld styleguidist build",
    "build:lib:icons": "svg-icon-generate --keepFillColor --svgDir src/atoms/icons/svg --destination src/atoms/icons/index.js --template src/atoms/icons/index.template && node ./npm_scripts/fixIconClasses.js",
    "precommit": "npm run lint:staged",
    "prestart": "npm run build:lib:icons && npm run stylus:generate-vars",
    "prepublish": "npm run build:lib:icons && npm run stylus:generate-vars",
    "postinstall": "npm run build:lib:icons",
    "test": "cross-env BABEL_ENV=test jest --projects jest.config.js jest-eslint.config.js",
    "test:acceptance": "codeceptjs run --steps",
    "lint": "npm run lint:styles && npm run lint:js",
    "lint:staged": "lint-staged",
    "lint:styles": "stylint src",
    "lint:js": "jest --projects jest-eslint.config.js",
    "deploy:dev": "node npm_scripts/deploy.js",
    "deploy:dev-local": "node npm_scripts/deploy.js --local",
    "build:examples": "npm run build:lib && webpack --config examples/webpack.config.js",
    "start:examples": "npm run build:lib && webpack --config examples/webpack.config.js && node examples/server.js",
    "hmr:examples": "npm run build:lib && webpack-dev-server --config examples/webpack.config.js --inline --hot --content-base examples --host 0.0.0.0",
    "release": "./node_modules/.bin/release",
    "stylus:generate-vars": "stylus -u foovar src/themes src/style/constants/breakpoints.styl"
  },
  "dependencies": {
    "cookies-js": "1.2.3",
    "flexibility": "2.0.1",
    "foovar": "0.6.2",
    "keycode": "2.1.9",
    "lodash.debounce": "4.0.8",
    "luxon": "0.4.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-sticky": "6.0.1",
    "react-svg-icon-generator": "0.4.0",
    "react-transition-group": "1.2.0",
    "recompose": "0.26.0",
    "styled-components": "3.3.3",
    "what-input": "4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-angular": "6.1.3",
    "@commitlint/core": "6.1.3",
    "@dc/eslint-config": "3.0.1",
    "@dc/ui-library-release": "file:./npm_scripts/release",
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "body-parser": "1.18.2",
    "codeceptjs": "1.1.0",
    "concurrently": "3.5.1",
    "core-js": "2.5.3",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-codeceptjs": "0.4.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "jest": "22.0.1",
    "jest-css-modules": "1.1.0",
    "jest-runner-eslint": "0.3.0",
    "jest-styled-components": "5.0.1",
    "jsdom": "11.5.1",
    "lint-staged": "7.0.5",
    "mock-local-storage": "1.0.5",
    "postcss-loader": "2.0.9",
    "raf": "3.4.0",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.2.0",
    "react-styleguidist": "7.0.10",
    "react-test-renderer": "16.2.0",
    "require-hacker": "3.0.1",
    "standard-version": "4.3.0",
    "style-loader": "0.19.1",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "svg-jsx-loader": "0.0.17",
    "url-join": "2.0.2",
    "url-loader": "^0.6.2",
    "webdriverio": "4.9.11",
    "webpack": "3.10.0",
    "webpack-modernizr-loader": "4.0.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "publishConfig": {
    "registry": "https://artifactory.dsg-i.com/api/npm/npm-virtual/"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "node npm_scripts/fix_changelog.js"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  }
}
